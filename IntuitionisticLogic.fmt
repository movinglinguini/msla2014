% directives for intuitionistic logic connectives

%format  ~>  = "\Rightarrow"
%format _~>_ = "\_\!\Rightarrow\!\_"
%format  ⊗   = "\times"
%format _⊗_  = "\_\!\!\times\!\!\_"

%format Vec.lookup  = lookup
%format Vec-exch    = exch
%format Product.map = map

%format TypeIL = Type "^{IL}"
%format ⊢IL = ⊢ "^{IL}"
%format ⟧IL = ⟧ "^{IL}"